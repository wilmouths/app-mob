generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl", "darwin", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  email String  @unique
  name  String?
}
